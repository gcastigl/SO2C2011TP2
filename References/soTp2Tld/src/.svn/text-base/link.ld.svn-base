/*
*/
ENTRY (_loader)

SECTIONS
{
   . = 0x00100000; /* 1 Mb */

   .text :  
   {
       text = .;
       code = .;
       *(.text)
   }

   .rodata ALIGN (0x1000) :  /* alineado a 4kb */ 
   {
       *(COMMON)
       *(.rodata)
   }

   .data ALIGN (0x1000) :   /* alineado a 4kb */
   {
       data = .;
       *(COMMON)
       *(.data)
   }

   .bss :    
   {
       sbss = .;
       *(COMMON)
       *(.bss)
       _ebss = .;
   }
    end = .; _end = .; __end = .; /* proximo lugar de memoria libre Next Free Address ( nf_address ) */
}


